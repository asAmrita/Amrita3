FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      F;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;  // starting initial values
  
boundaryField
{
    
    top
    {
        type  fixedValue;
        value uniform 100;
    }

    bottom
    {
		 type fixedValue;
          value uniform 50;
    }

    left
    {
        type zeroGradient;
    }

    right
    {
        type zeroGradient;
    }

    frontAndBack
	{
        type empty;
    }




}

